.i-panel {
  position: relative;
  overflow: hidden
}

.i-panel-title {
  font-size: 14px;
  line-height: 1;
  color: #1c2438;
  padding: 10px 15px 6px;
}

.i-panel-content {
  position: relative;
  background: #fff;
  overflow: hidden;
  padding: 0 15px 20px; 
  font-size: 26px;
}

.i-panel-content-text {
  background: #5cadff;
  color: white;
  padding: 0 4px;
}

.i-panel-content::after {
  content: '';
  position: absolute;
  bottom: 0;
  left: 15px;
  right: 0;
  height: 1px;
  background-color: #e9eaec;
}

