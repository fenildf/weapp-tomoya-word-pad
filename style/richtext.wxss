.rich-content {
  font-size: 14px;
  white-space: pre;
  color: #495060;
  white-space: normal;
  word-wrap: break-word;
}

.rich-subtitle {
  padding: 20px 0 6px;
  color: #1c2438;
  white-space: normal;
  word-wrap: break-word;
}

.rich-footer {
  padding-top: 30px;
  font-size: 10px;
  display: flex;
  align-items: center;
  color: #80848f;
  white-space: normal;
  word-wrap: break-word;
}

.rich-error {
  font-size: 14px;
  color: #80848f;
  white-space: normal;
  word-wrap: break-word;
}